{
  "expo": {
    "name": "econominator",
    "slug": "econominator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/wallet_icon.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notif-icon.png",
          "color": "#DF73FF"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/notif-icon.png",
        "backgroundColor": "#DF73FF"
      }
    },
    "packagerOpts": {
      "dev": true
    },
    "web": {
      "favicon": "./src/assets/wallet_icon.png"
    }
  }
}
